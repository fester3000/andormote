void check() {
	float z;
	z = 12.54;
	z = .12;
	z = 0.01;
	z = 12.0;
	
	int a;
	a = 15;
	a = 'a' + 3 + 4.2;
	int b;
	b = 234;

	if (a < b) {
		device camera;
		camera.getDevice("CAMERA");
		camera.setParam("FOCUS", "AUTO");
		camera.setParam("EXPOSURE", a);
		camera.setParam("RECORD", TRUE);
	}
	if (a < b) {
		device camera2;
		camera2.getDevice("CAMERA");
		camera2.setParam("FOCUS", "AUTO");
		camera2.setParam("EXPOSURE", b);
		camera2.setParam("RECORD", "TRUE");
	} elseif(b > a) {
		a = 13;
	} else {
		return 5;
	}
}
